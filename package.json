{
  "name": "avalancheuk",
  "version": "2.0.0",
  "author": "Ben Edwards <hello@benedfit.com>",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start": "harp server ./source",
    "build": "harp compile ./source ./_output",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha test",
    "lint": "eslint -f unix .",
    "inspect": "jsinspect .",
    "depcheck": "david",
    "pretest": "npm run build && npm run lint && npm run inspect && npm run depcheck",
    "test": "npm run coverage",
    "posttest": "(istanbul check-coverage --statements 90 --branches 90 --functions 100 --lines 90 && rm -rf coverage) || echo Look at 'coverage/lcov-report/index.html' to find out more"
  },
  "dependencies": {
    "harp": "^0.20.1"
  },
  "devDependencies": {
    "david": "^7.0.1",
    "eslint": "^2.3.0",
    "eslint-config-clock": "^1.0.2",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.1",
    "istanbul": "^0.4.2",
    "jsinspect": "^0.8.0",
    "mocha": "^2.4.5"
  }
}
