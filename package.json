{
  "name": "avalancheuk",
  "version": "2.0.0",
  "author": "Ben Edwards <hello@benedfit.com>",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start": "pliers go -o",
    "deploy": "bash ./deploy.sh",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha test",
    "lintJs": "eslint -f unix .",
    "lintJade": "pug-lint .",
    "lintStylus": "stylint .",
    "inspect": "jsinspect .",
    "depcheck": "david",
    "pretest": "pliers build && npm run lintJs && npm run inspect && npm run lintStylus && npm run lintJade && npm run depcheck",
    "test": "npm run coverage",
    "posttest": "(istanbul check-coverage --statements 90 --branches 90 --functions 100 --lines 90 && rm -rf coverage) || echo Look at 'coverage/lcov-report/index.html' to find out more",
    "precommit": "npm run lintJs && npm run inspect && npm run lintStylus && npm run lintJade"
  },
  "dependencies": {
    "any-newer-files": "0.0.4",
    "browser-sync": "^2.12.3",
    "configury": "^0.3.0",
    "glob": "^7.0.3",
    "harp": "^0.20.1",
    "rfg-api": "^0.1.7"
  },
  "devDependencies": {
    "david": "^7.0.1",
    "eslint": "^2.3.0",
    "eslint-config-clock": "^1.0.2",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.1",
    "husky": "^0.11.4",
    "istanbul": "^0.4.2",
    "jsinspect": "^0.8.0",
    "mocha": "^2.4.5",
    "pliers": "^1.2.1",
    "pliers-imagemin": "^3.0.1",
    "pug-lint": "^2.1.6",
    "stylint": "^1.3.8"
  }
}
