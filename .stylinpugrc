{ "ignoreFiles": []

, "stylusWhitelist":
  /* Modernizr classes */
  [ "*js"
  , "*touchevents"

  /* State classes */
  , "is-*"
  , "has-*"

  /* Spreadshirt */
  , "sprd-*"
  , "Sprd*"
  , "custom*"
  ]

, "pugWhitelist":
  /* js-* classes ONLY allowed in Pug */
  [ "js-*"
  , "no-js"
  ]

, "stylusBlacklist":
  /* js-* classes NOT allowed in Stylus */
  [ "js-*"
  ]

, "pugBlacklist": []
}
